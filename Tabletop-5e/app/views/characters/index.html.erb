<% provide(:title, "Character List") %>
<h1>Character Index</h1>
<ul>
  <% Character.all.each do |c| %>
    <li><%= link_to(c.name, c) + ", Level " + exp_to_level(c.experience_points).to_s + ", " + c.race + ", " + c.character_class %> <%= link_to "Delete Character", character_path(id: c.id), method: :delete %></li>
  <% end %>
</ul>